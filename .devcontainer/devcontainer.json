{
  "name": "Ruby",
  "dockerComposeFile": "docker-compose.yml",
  "service": "ruby",
  "workspaceFolder": "/workspace",
  "settings": {
    "ruby.format": "rufo",
    "ruby.useLanguageServer": true
  },
  "extensions": [
    "rebornix.Ruby",
    "redhat.vscode-yaml",
    "kaiwood.endwise",
    "VisualStudioExptTeam.vscodeintellicode",
    "esbenp.prettier-vscode",
    "github.copilot"
  ]
}